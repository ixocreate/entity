{
    "name": "kiwi-suite/entity",
    "description": "kiwi-suite/entity is a library for entities and collections",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Thomas Payer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "zendframework/zend-filter": "^2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0.6",
        "friendsofphp/php-cs-fixer": "^2.0",
        "kiwi-suite/servicemanager": "dev-develop",
        "kiwi-suite/application": "dev-develop",
        "kiwi-suite/contract": "dev-develop",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Entity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Entity\\": "tests/src/",
            "IxocreateMisc\\Entity\\": "tests/misc/"
        }
    }
}
