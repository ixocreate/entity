{
    "name": "ixocreate/entity",
    "description": "A library for entities and collections",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "IXOCREATE"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.1",
        "ixocreate/contract": "=1.0.0-alpha1",
        "ixocreate/servicemanager": "=1.0.0-alpha1",
        "ixocreate/application": "=1.0.0-alpha1",
        "zendframework/zend-filter": "^2.7"
    },
    "require-dev": {
        "ixocreate/coding-standard": "^1.0.1",
        "phpunit/phpunit": "^6.0.6",
        "php-coveralls/php-coveralls": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Ixocreate\\Entity\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IxocreateTest\\Entity\\": "tests/src/",
            "IxocreateMisc\\Entity\\": "tests/misc/"
        }
    },
    "scripts": {
        "fix-cs": "vendor/friendsofphp/php-cs-fixer/php-cs-fixer fix --config vendor/ixocreate/coding-standard/.php_cs"
    }
}
